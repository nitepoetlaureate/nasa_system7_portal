{
  "name": "nasa-system7-client",
  "version": "1.0.0",
  "description": "React frontend for the NASA System 7 Portal - Authentic System 7 interface with NASA data visualization",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.6.0",
    "d3": "^7.8.5",
    "framer-motion": "^10.16.16",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.20.1",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4",
    "lodash": "^4.17.21",
    "react-intersection-observer": "^9.5.3",
    "react-window": "^1.8.8",
    "react-virtualized-auto-sizer": "^1.0.20"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --coverage --watchAll=false --ci",
    "test:legacy": "react-scripts test",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "eject": "react-scripts eject",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "analyze": "npm run build && npx bundle-analyzer build/static/js/*.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-unused-vars": "warn",
      "no-console": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.22.0",
    "@cypress/react": "^9.0.1",
    "@cypress/webpack-dev-server": "^5.1.3",
    "autoprefixer": "^10.4.16",
    "babel-jest": "^29.7.0",
    "cypress": "^15.5.0",
    "eslint": "^8.54.0",
    "eslint-config-react": "^1.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-watch-typeahead": "^2.2.2",
    "msw": "^2.0.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.0",
    "tailwindcss": "^3.3.6",
    "webpack-bundle-analyzer": "^4.9.1",
    "whatwg-fetch": "^3.6.0"
  },
  "keywords": [
    "react",
    "nasa",
    "system7",
    "retro",
    "space",
    "astronomy",
    "education",
    "d3",
    "visualization"
  ],
  "author": "NASA System 7 Portal Team",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "proxy": "http://localhost:3001"
}
