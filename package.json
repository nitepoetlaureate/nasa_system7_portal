{
  "name": "nasa-system7-portal",
  "version": "1.0.0",
  "description": "A nostalgic web application that brings NASA's vast collection of space data to life through an authentic Apple System 7 interface.",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm start",
    "client": "cd client && npm start",
    "install-all": "npm install && cd server && npm install && cd ../client && npm install",
    "build": "cd client && npm run build",
    "start": "cd server && npm start"
  },
  "keywords": [
    "nasa",
    "space",
    "react",
    "nodejs",
    "system7",
    "retro",
    "education",
    "astronomy"
  ],
  "author": "NASA System 7 Portal Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/nasa-system7-portal.git"
  },
  "dependencies": {
    "draggabilly": "^3.0.0"
  },
  "devDependencies": {
    "concurrently": "^7.6.0"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  }
}
